{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "comma-dangle": [
      2,
      "only-multiline"
    ]
  },
  "env": {
    "mocha": true
  },
  "parserOptions": {
    "sourceType": "script"
  },
  "extends": "eslint-config-es5",
  "globals": {
      "angular": true,
      "inject": true
  }
}
